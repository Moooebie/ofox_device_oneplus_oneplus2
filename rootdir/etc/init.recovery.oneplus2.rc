on fs
    wait /dev/block/platform/soc.0/f9824900.sdhci
    symlink /dev/block/platform/soc.0/f9824900.sdhci /dev/block/bootdevice

 on recovery
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1550000
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 1550000
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 1550000
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 1550000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 1550000
    write /sys/devices/system/cpu/cpu5/cpufreq/scaling_max_freq 1550000
    write /sys/devices/system/cpu/cpu6/cpufreq/scaling_max_freq 1550000
    write /sys/devices/system/cpu/cpu7/cpufreq/scaling_max_freq 1550000
    

    setprop sys.usb.ffs.aio_compat
    

